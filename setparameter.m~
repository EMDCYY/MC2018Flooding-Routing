function [sf, po, zr, wr, zt, wt,lqd, zw, zs, ww, wf] = setparameter(x)
% sf =  sourcefile;
% po = position;
% zr = zigbee communicatoin range;
% wr = wifi communicatoin range;
% zt = zigbee routing table;
% wt = wifi routing table;
% lqd = link quality deploy
% zw = zigbee wake duration
% zs = zigbee sleep druatoin
% ww = wifi work duration
% wf = wifi traffic

    sf = stdsourcefile(x);
    po = std20point(x);
    zr = 6;
    wr = 20;
    zt = findrouting(position, zigcomrange);
    fig1 = routingfigure(zr, po, zt);
    wt = findrouting(position, wr);
    fig2 = routingfigure(wr, po, wt);
    lqd = stdlinkdeploy(x);
    zw = 10;
    zs = 100;
    ww = 2;
    wf = 0.8;
    
    
end